<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- Setter injection using "value" attribute -->
	<bean id="personBean" class="com.spring.examples.Person">
		<property name="firstName">
			<null />
		</property>
		<property name="lastName" value="#{null}"></property>
		<property name="age" value="28"></property>
		<property name="card" ref="creditCardBean"></property>
	</bean>

	<bean id="creditCardBean" class="com.spring.examples.CreditCard"
		p:cardNo="#{null}"
		p:bankName=""
		p:cvv="707" />

</beans>